    "benchmark": "^2.1.4",
    "decoders": "^2.4.0",
    "heapdump": "^0.3.15",
    "joi": "^17.13.1",
    "nodemon": "^3.1.0",
    "objectmodel": "^4.4.5",
    "runtypes": "^6.7.0",
    "superstruct": "^1.0.4",
